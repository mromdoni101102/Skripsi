"use strict";

// ======================================================================
// BAGIAN 1: MENYIAPKAN "PROFILE PALSU" (INI PERBAIKANNYA)
// ======================================================================
// Kita beritahu Jest: "Aku tahu kode Gallery butuh './profile.js'.
// Ini adalah versi palsunya, gunakan ini."
_getJestObj().mock('./profile.js', () => {
  return function MockedProfile() {
    // Kita beri tanda pengenal agar bisa kita hitung di dalam tes.
    return /*#__PURE__*/(0, _jsxRuntime.jsx)("div", {
      "data-testid": "profile-component"
    });
  };
}, {
  virtual: true
});

// ======================================================================
// BAGIAN 2: MENGAMBIL KODE MAHASISWA
// ======================================================================
var _jsxRuntime = require("react/jsx-runtime");
function _getJestObj() {
  const {
    jest
  } = require("@jest/globals");
  _getJestObj = () => jest;
  return jest;
}
/**
 * @jest-environment jsdom
 */
require('@testing-library/jest-dom');
const React = require('react');
const {
  render,
  screen
} = require('@testing-library/react');
if (!process.env.SUBMISSION_PATH) {
  throw new Error('SUBMISSION_PATH environment variable not set.');
}
const submission = require(process.env.SUBMISSION_PATH);
const Gallery = submission.Gallery || submission.default || submission;

// ======================================================================
// BAGIAN 3: CHECKLIST PENILAIAN YANG SUDAH DIPERBAIKI
// ======================================================================
describe('Praktikum: Komponen Gallery', () => {
  beforeEach(() => {
    render(/*#__PURE__*/(0, _jsxRuntime.jsx)(Gallery, {}));
  });
  test('Kriteria 1: Harus menampilkan sebuah section atau div sebagai pembungkus utama', () => {
    const {
      container
    } = render(/*#__PURE__*/(0, _jsxRuntime.jsx)(Gallery, {}));
    // container.firstChild adalah elemen paling luar yang dirender
    expect(container.firstChild).toBeInTheDocument();
  });
  test('Kriteria 2: Harus me-render TEPAT 3 komponen Profile (tiruan)', () => {
    // Sekarang kita bisa mencari komponen tiruan kita berdasarkan 'data-testid'.
    const profileMocks = screen.getAllByTestId('profile-component');
    expect(profileMocks).toHaveLength(3);
  });
  test('Kriteria 3: Komponen "Gallery" harus diexport dengan benar', () => {
    expect(Gallery).toBeDefined();
  });
});
