"use strict";

// ======================================================================
// BAGIAN 1: PERSIAPAN LINGKUNGAN
// ======================================================================

// Kita TETAP perlu membuat tiruan dari 'next/image' karena kita tidak
// menjalankan ini di dalam lingkungan Next.js. Ini sudah benar.
_getJestObj().mock('next/image', () => {
  // eslint-disable-next-line @next/next/no-img-element
  return props => /*#__PURE__*/(0, _jsxRuntime.jsx)("img", {
    ...props
  });
});

// ======================================================================
// BAGIAN 2: MENGAMBIL KODE MAHASISWA
// ======================================================================
var _jsxRuntime = require("react/jsx-runtime");
function _getJestObj() {
  const {
    jest
  } = require("@jest/globals");
  _getJestObj = () => jest;
  return jest;
}
/**
 * @jest-environment jsdom
 */
require('@testing-library/jest-dom');
const React = require('react');
const {
  render,
  screen
} = require('@testing-library/react');
if (!process.env.SUBMISSION_PATH) {
  throw new Error('SUBMISSION_PATH environment variable not set.');
}
// Logika cerdas ini untuk mengambil komponen Gallery tetap kita gunakan.
const submission = require(process.env.SUBMISSION_PATH);
const Gallery = submission.Gallery || submission.default || submission;

// ======================================================================
// BAGIAN 3: "CHECKLIST" PENILAIAN FUNGSIONAL YANG DISEMPURNAKAN
// ======================================================================
describe('Praktikum: Komponen Gallery', () => {
  // Render komponen mahasiswa sebelum setiap tes dijalankan
  beforeEach(() => {
    // Pastikan kita me-render komponen Gallery yang sudah kita ambil, bukan yang lain.
    render(/*#__PURE__*/(0, _jsxRuntime.jsx)(Gallery, {}));
  });
  test('Kriteria 1: Harus menampilkan judul "Ilmuwan Luar Biasa"', () => {
    // Tes ini mencari heading <h1>. Ini sudah bagus.
    const headingElement = screen.getByRole('heading', {
      level: 1,
      name: /ilmuwan luar biasa/i
    });
    expect(headingElement).toBeInTheDocument();
  });
  test('Kriteria 2: Harus me-render TEPAT 3 elemen gambar', () => {
    // Daripada mencari komponen tiruan, kita cari hasil akhirnya: yaitu gambar.
    // screen.getAllByRole('img') akan mencari semua tag <img> yang ada di layar.
    const images = screen.getAllByRole('img');
    expect(images).toHaveLength(3);
  });
  test('Kriteria 3: Semua gambar yang dirender harus memiliki alt text "Katherine Johnson"', () => {
    // Kita juga bisa mencari berdasarkan alt text. Jika ada 3, berarti benar.
    const imagesWithCorrectAlt = screen.getAllByAltText('Katherine Johnson');
    expect(imagesWithCorrectAlt).toHaveLength(3);
  });
  test('Kriteria 4: Komponen "Gallery" harus diexport dengan benar', () => {
    expect(Gallery).toBeDefined();
  });
});
